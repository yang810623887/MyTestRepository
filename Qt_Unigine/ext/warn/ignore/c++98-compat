#if !defined(WARN_INCLUDE_PUSH)
#   error "`warn/ignore/c++98-compat` used without `warn/push`"
#endif

#if defined(WARN_IGNORE_CXX98_COMPAT)
#   error "`warn/ignore/c++98-compat` already included"
#endif

#define WARN_IGNORE_CXX98_COMPAT

#if defined(__clang__)
#   if __clang_major__ > 3 || (__clang_major__ == 3  && __clang_minor__ > 2)
#       if __has_warning("-Wc++98-compat")
#           pragma clang diagnostic ignored "-Wc++98-compat"
#       endif
#   endif
#elif defined(__GNUC__)
//  Not available
#elif defined(_MSC_VER)
//  Not available
#endif