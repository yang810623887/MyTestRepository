<base_material version="2.0" name="mesh_base" editable="0" parameters_prefix="m" defines="VERTEX_ATTRIBUTE_GEOMETRY">
	
	<state name="ambient_light" internal="1">1</state>
	
	<state name="alpha_fade"	defines="name"	internal="1"/>
	<state name="spline"		defines="name"	internal="1"/>
	
	<state name="dynamic"		vegetation="0"											defines="name"	internal="1"/>
	<state name="static_shadow"	vegetation="0"	parallax="0"	geometry_inflation="0"	defines="name"	internal="1"/>
	
	<group name="Default">
		<state name="workflow" items="metalness,specular" defines="items"			tooltip="&lt;p&gt;Workflow selection determines which textures are to be used.&lt;/p&gt;&lt;p&gt;There are two workflows available:&lt;ul&gt;&lt;li&gt;&lt;b&gt;Metalness&lt;/b&gt; — new standard of textures (albedo, metalness, roughness).&lt;/li&gt;&lt;li&gt;&lt;b&gt;Specular&lt;/b&gt; — old standard of textures (diffuse, specular, gloss).&lt;/li&gt;&lt;/ul&gt;&lt;/p&gt;&lt;p&gt;It is strongly recommended to use the Metalness workflow as it uses a less number of texture channels, and uses material parameters more similar to real ones.&lt;/p&gt;"/>
		<state name="deferred" title="Deferred Buffers" 							tooltip="&lt;p&gt;Enable filling of deferred buffers.&lt;/p&gt;&lt;p&gt;Used by transparent objects to participate in post-effects.&lt;/p&gt;">1</state>
		<state name="multiple_environment_probes" transparent="2" defines="name" 	tooltip="&lt;p&gt;Illuminate a transparent object with several environment probes and map their cubemaps to this object.&lt;/p&gt;">0</state>
	</group>
	
	<group name="Options">
		<state name="ao_map"								defines="name"	pass_defines="deferred,forward"	title="Ambient Occlusion" tooltip="&lt;p&gt;Enable the Ambient Occlusion effect.&lt;/p&gt;&lt;p&gt;This effect is used to modulate global environment illumination using baked shadows from environment probe light sources.&lt;/p&gt;"/>
		<state name="specular_map"		workflow="0"		defines="name"	pass_defines="deferred,forward"							tooltip="&lt;p&gt;Use B-channel of the &lt;b&gt;Shading Texture&lt;/b&gt; as a specular reflection intensity map.&lt;/p&gt;"/>
		<state name="microfiber_map"						defines="name"	pass_defines="deferred,forward"							tooltip="&lt;p&gt;Use A-channel of the &lt;b&gt;Shading Texture&lt;/b&gt; as a microfiber map.&lt;/p&gt;"/>
		<state name="translucent_map"	transparent="0,1"	defines="name"	pass_defines="deferred,forward"							tooltip="&lt;p&gt;Enable the translucency effect.&lt;/p&gt;&lt;p&gt;This effect is used to create translucent materials, that allow light to pass through.&lt;/p&gt;"/>
		
		<state name="detail"	defines="name"	pass_defines="deferred,transparent"	tooltip="&lt;p&gt;Enable detail rendering.&lt;/p&gt;&lt;p&gt;This effect can be used to add details for objects, that are close to camera, or variations for distant ones, to make them look more realistic.&lt;/p&gt;"/>
		
		<state name="emission"			defines="name"	pass_defines="ambient,deferred"					tooltip="&lt;p&gt;Enable emission rendering.&lt;/p&gt;&lt;p&gt;This effect simulates glow from extremely bright surfaces and can be used to create self-luminous objects (lamps, fire, etc.)&lt;/p&gt;"/>
		<state name="emission_bake_gi"	defines="name"	pass_defines="ambient,emission"				emission="1"	tooltip="">1</state>
		<state name="cross_section"		defines="name"	pass_defines="deferred,ambient,emission"	emission="1"	tooltip=""/>
		
		<state name="lightmap"	defines="name"	pass_defines="deferred,ambient"		tooltip="&lt;p&gt;Enable use of a lightmap.&lt;/p&gt;&lt;p&gt;Activates an additional light map texture specifying that the material will be lit by it.&lt;/p&gt;"/>
		<state name="bevel"		defines="name"	pass_defines="deferred"				tooltip=""/>
		<state name="auxiliary"	defines="name"	pass_defines="auxiliary"			tooltip="&lt;p&gt;Enable auxiliary rendering pass for the material.&lt;/p&gt;&lt;p&gt;Can be used for custom post-effects, such as thermal vision, etc.&lt;/p&gt;"/>
		
		<state name="parallax"	transparent="0,1"	defines="name"	pass_defines="deferred,emission,shadow,depth_pre_pass"	tooltip="&lt;p&gt;Enable parallax occlusion mapping.&lt;/p&gt;&lt;p&gt;Can be used to fake surface relief adding more realism with less influence on the performance.&lt;/p&gt;"/>
		
		<state name="vertex_color"		defines="name"	pass_defines="deferred,transparent,emission"	tooltip="&lt;p&gt;Use vertex colors for per-channel modulation of the albedo, AO, light map, emission, detail and auxiliary textures.&lt;/p&gt;&lt;p&gt;Can be used to reduce the number of textures and broaden the diversity of materials.&lt;/p&gt;"/>
		<state name="planar_reflection"	defines="name"	pass_defines="ambient"						tooltip="&lt;p&gt;Enable rendering of dynamic reflections.&lt;/p&gt;&lt;p&gt;Using this option you can create realistic flat mirrors and flat reflective surfaces (parquet, flat varnished surfaces, etc.)&lt;/p&gt;"/>
		<state name="vegetation"		defines="name"												tooltip="&lt;p&gt;Use the material for vegetation simulation.&lt;/p&gt;&lt;p&gt;Enabling this option allows you to create materials for vegetation crowns and trunks, that can be animated to add more realism.&lt;/p&gt;"/>
		
		<state name="calculate_tangent_space"	defines="name"	pass_defines="object"	tooltip="&lt;p&gt;Enables calculation of the tangent space in the shader.&lt;/p&gt;&lt;p&gt;Can be used for materials applied to geometry with incorrect tangents or to geometry without tangents at all.&lt;/p&gt;"/>
		
		<state name="jitter_transparency"		defines="name"	pass_defines="object"	transparent="1"	/>
		
		<state name="lightmap_with_ambient"		defines="name"	pass_defines="deferred,ambient"	tooltip="&lt;p&gt;Allows the light map to take ambient lighting into account.&lt;/p&gt;&lt;p&gt;When unchecked, only the light map is rendered.&lt;/p&gt;">0</state>
		
		<state name="mip_bias"					defines="name"	pass_defines="object"	tooltip=""/>
		
		<state name="normal_map"			items="tangent_space,object_space"	defines="name_items"	pass_defines="object"/>
		<state name="geometry_inflation"	items=",wire,balloon"				defines="items"			tooltip="&lt;p&gt;Makes it possible to bloat the geometry of objects to make them visible from a distant position.&lt;/p&gt;&lt;p&gt;Can be used for wires and sphere-shaped objects.&lt;/p&gt;"/>
		
		<state name="opacity_map"			transparent="1,2"	items="main,normal"	defines="name_items"	pass_defines="object"	tooltip="&lt;p&gt;Texture source for opacity map: A-channel of the diffuse/albedo texture or B-channel of the normal map&lt;/p&gt;"/>
		
		<state name="refraction"			transparent="2"											tooltip="&lt;p&gt;Enable rendering of post-processing refraction.&lt;/p&gt;&lt;p&gt;This effect makes transparent object look more realistic by distorting an image behind it.&lt;/p&gt;"/>
		<state name="transparent_blur"		transparent="2"											tooltip="&lt;p&gt;Enable rendering of transparent blur effect.&lt;/p&gt;&lt;p&gt;This effect makes it possible to create transparent matte objects (e.g. matte glass).&lt;/p&gt;"/>
		<state name="transparent_shadow"	transparent="2"	defines="name"	pass_defines="shadow"	/>
	</group>
	
	<group name="Detail">
		<state name="detail_blend"			detail="1"		items="alpha_blend,overlay,multiply"			defines="name_items"	pass_defines="deferred,transparent"	title="Blending"	tooltip="&lt;p&gt;Blending mode: determines how base and detail textures are blended.&lt;/p&gt;"/>
		<state name="detail_angle_fade"		detail="1"		items=",detail_angle_fade0,detail_angle_fade1"	defines="items"			pass_defines="deferred,transparent"	title="Angle Fade"	tooltip="&lt;p&gt;Overlapping mode for base textures:&lt;ul&gt;&lt;li&gt;overlap the base material by using the object transformation (detail textures will be rotated with the mesh).&lt;/li&gt;&lt;li&gt;World transform (overlap) - overlap the base textures by using the world transformation (no matter if you rotate the object, the detail textures will always be projected atop).&lt;/li&gt;&lt;/ul&gt;&lt;/p&gt;"/>
		<state name="detail_mask"			detail="1"														defines="name"			pass_defines="deferred,transparent"	title="Mask"		tooltip="&lt;p&gt;Use a texture mask for blending base and detail materials selectively (the detail textures will be shown according to the mask).&lt;/p&gt;"/>
	</group>
	
	<group name="UV mapping">
		<state name="base_mapping"							items="uv0,uv1,triplanar"								defines="name_items"										title="Base"				tooltip="UV map source for base textures"/>
		<state name="detail_mapping"		detail="1"		items="base,uv0,uv1,triplanar,overlap0,overlap1"		defines="name_items"	pass_defines="deferred,transparent"	title="Detail"				tooltip="UV map source for detail textures">1</state>
		<state name="detail_mask_mapping"	detail="1"		detail_mask="1"	items="base,detail,uv0,uv1,triplanar,overlap0,overlap1"	defines="name_items"	pass_defines="deferred,transparent"	title="Detail Mask"		tooltip="UV map source for the detail blend mask texture"/>
		<state name="ao_mapping"			ao_map="1"		items="base,uv0,uv1"									defines="name_items"	pass_defines="deferred,forward"		title="Ambient Occlusion"	tooltip="UV map source for the ambient occlusion texture">1</state>
		<state name="lightmap_mapping"		lightmap="1"	items="base,uv0,uv1"									defines="name_items"	pass_defines="deferred,ambient"		title="Lightmap"			tooltip="UV map source for the lightmap occlusion texture">2</state>
		<state name="bevel_mapping"			bevel="1"		items="uv0,uv1,triplanar"								defines="name_items"	pass_defines="deferred"				title="Bevel"				tooltip="UV map source for the bevel texture">0</state>
	</group>
	
	<group name="Parallax">
		<state name="parallax_shadow"		parallax="1"	transparent="0,1"	defines="name"	pass_defines="deferred,emission,shadow,depth_pre_pass"	title="Shadow"			tooltip="&lt;p&gt;Enable shadows for surfaces with parallax maps.&lt;/p&gt;&lt;p&gt;This effect makes the relief look more realistic.&lt;/p&gt;"/>
		<state name="parallax_depth_cutout"	parallax="1"	transparent="0,1"	defines="name"	pass_defines="deferred,emission,shadow,depth_pre_pass"	title="Depth Cutout"	tooltip="&lt;p&gt;Enable the depth cutout effect providing smooth alignment with real surface geometry.&lt;/p&gt;&lt;p&gt;This effect makes a plane look like real geometry. It also improves the SSAO calculation on surfaces with parallax maps.&lt;/p&gt;"/>
		<state name="parallax_cutout"		parallax="1"	transparent="0,1"	defines="name"	pass_defines="deferred,emission,shadow,depth_pre_pass"	title="Cutout"			tooltip="&lt;p&gt;Enable the cutout effect providing smooth edges between the opaque and transparent areas.&lt;/p&gt;"/>
	</group>
	
	<group name="Vertex Color">
		<state name="vertex_albedo"			workflow="0"	vertex_color="1"	items=",rgb"		defines="name"			pass_defines="deferred,forward"		title="Albedo"				tooltip="Use vertex color for albedo texture"/>
		<state name="vertex_diffuse"		workflow="1"	vertex_color="1"	items=",rgb"		defines="name"			pass_defines="deferred,forward"		title="Diffuse"				tooltip="Use vertex color for diffuse texture"/>
		<state name="vertex_ao"								vertex_color="1"	items=",r,g,b,a"	defines="name_items"	pass_defines="deferred,forward"		title="Ambient Occlusion"	tooltip="Use vertex color for the ambient occlusion texture"/>
		<state name="vertex_lightmap"						vertex_color="1"	items=",rgb"		defines="name"			pass_defines="deferred,ambient"		title="Light Map"			tooltip="Use vertex color for the lightmap texture"/>
		<state name="vertex_detail_mask"	detail="1"		vertex_color="1"	items=",r,g,b,a"	defines="name_items"	pass_defines="deferred,transparent"	title="Detail Mask"			tooltip="Use vertex color for the detail mask texture"/>
		<state name="vertex_emission"		emission="1"	vertex_color="1"	items=",rgb"		defines="name"			pass_defines="emission"				title="Emission"			tooltip="Use vertex color for the emission texture"/>
		<state name="vertex_auxiliary"		auxiliary="1"	vertex_color="1"	items=",rgb"		defines="name"			pass_defines="auxiliary"			title="Auxiliary"			tooltip="Use vertex color for the auxiliary texture"/>
	</group>
	
	<group name="Vegetation">
		<state name="animation"				vegetation="1"	items=",default,field"		defines=",ANIMATION,FIELD_ANIMATION"							tooltip="&lt;p&gt;Mode of procedural animation of wind affecting vegetation.&lt;/p&gt;&lt;p&gt;Determines if animation is enabled and controlled by Vegetation animation parameters only, or by a &lt;b&gt;FieldAnimation&lt;/b&gt; node.&lt;/p&gt;"/>
		<state name="leafs"					vegetation="1"	items=",geometry,billboard"	defines="name_items"					title="Leaves"			tooltip="&lt;p&gt;Leaves rendering mode&lt;/p&gt;&lt;p&gt;Determines the way the geometry of leaves is rendered. Geometry mode provides more realism, but it is less performance-friendly.&lt;/p&gt;"/>
		<state name="angle_fade"			vegetation="1"	transparent="1,2"			defines="name"													tooltip="&lt;p&gt;Enables cutting out surfaces at a certain angle between the surface normal and the camera view direction vector&lt;/p&gt;&lt;p&gt;This option can be used to gain performance&lt;/p&gt;"/>
		<state name="shadow_offset_state"	vegetation="1"	pass_defines="shadow"		defines=",SHADOW_OFFSET"				title="Shadow Offset"	tooltip="&lt;p&gt;Offset parameter for leaves shadows.&lt;/p&gt;"/>
	</group>
	
	<group name="Spatial Color Noise">
		<state name="noise_2d" transparent="0,1" defines="name" pass_defines="deferred" title="2D Noise" tooltip="&lt;p&gt;Add spatial color noise based on 2D texture.&lt;/p&gt;&lt;p&gt;Can be used to add random color variation for leaves.&lt;/p&gt;"/>
		<state name="noise_3d" transparent="0,1" defines="name" pass_defines="deferred" title="3D Noise" tooltip="&lt;p&gt;Add spatial color noise based on 3D texture.&lt;/p&gt;&lt;p&gt;Can be used to add random color variation for leaves.&lt;/p&gt;"/>
	</group>
	
	<group name="Planar Reflection">
		<state name="reflection_size"		planar_reflection="1"	items="128,256,512,1024,2048,4096,quart_height,half_height,height"	title="Map Size"	tooltip="&lt;p&gt;Reflection map size, in pixels.&lt;/p&gt;"			>7</state>
		<state name="reflection_two_sided"	planar_reflection="1"																		title="Two Sided"	tooltip="&lt;p&gt;Enable two-sided planar reflection mode.&lt;/p&gt;"	>0</state>
		<state name="reflection_show_pivot"	planar_reflection="1"																		title="Show Pivot"	tooltip="&lt;p&gt;Show the reflection pivot point.&lt;/p&gt;"			>1</state>
		<state name="reflection_blur"		planar_reflection="1"	pass_defines="reflection"	defines="name"							title="Blur"																				>1</state>
	</group>
	
	<group name="Post Processing">
		<state name="material_ssao"					transparent="0,1"	deferred="1"	title="SSAO"				tooltip="&lt;p&gt;Enable the Screen Space Ambient Occlusion effect.&lt;/p&gt;">1</state>
		<state name="material_ssr"					transparent="0,1"	deferred="1"	title="SSR"					tooltip="&lt;p&gt;Enable the Screen Space Reflections effect.&lt;/p&gt;">1</state>
		<state name="material_sss"					transparent="0,1"	deferred="1"	title="SSS"					tooltip="&lt;p&gt;Enable the Subsurface Scattering effect.&lt;/p&gt;">0</state>
		<state name="material_dof"										deferred="1"	title="DOF"					tooltip="&lt;p&gt;Enable Depth Of Field effect.&lt;/p&gt;">1</state>
		<state name="material_motion_blur"								deferred="1"	title="Motion Blur"			tooltip="&lt;p&gt;Enable the Motion Blur effect.&lt;/p&gt;">1</state>
		<state name="material_sun_shafts"								deferred="1"	title="Sun Shafts"			tooltip="&lt;p&gt;Enable the Sun Shafts effect.&lt;/p&gt;">1</state>
		<state name="material_shoreline_wetness"	transparent="0,1"	deferred="1"	title="Shoreline Wetness"	tooltip="&lt;p&gt;Enable the wetness effect for objects near the shoreline.&lt;/p&gt;">1</state>
	</group>
	
	<group name="Light Passes">
		<state name="ambient"				transparent="2"	title="Ambient"				tooltip="&lt;p&gt;Enable rendering of the material illuminated by an environment probe light source.&lt;/p&gt;"	>1</state>
		<state name="environment_probe"		transparent="2"	title="Environment probe"	tooltip="&lt;p&gt;Enable rendering of the material illuminated by an environment probe light source.&lt;/p&gt;"	>1</state>
		<state name="voxel_probe"			transparent="2"	title="Voxel probe"			tooltip="&lt;p&gt;Enable rendering of the material illuminated by an voxel probe light source.&lt;/p&gt;"		>1</state>
		<state name="light_omni"			transparent="2"	title="Light Omni"			tooltip="&lt;p&gt;Enable rendering of the material illuminated by an omni light source.&lt;/p&gt;"				>1</state>
		<state name="light_proj"			transparent="2"	title="Light Proj"			tooltip="&lt;p&gt;Enable rendering of the material illuminated by a projected light source.&lt;/p&gt;"			>1</state>
		<state name="light_world"			transparent="2"	title="Light World"			tooltip="&lt;p&gt;Enable rendering of the material illuminated by a world light source.&lt;/p&gt;"				>1</state>
	</group>
	
	<group name="Shadows">
		<state name="soft_shadows"			transparent="2"	title="Soft Shadows"			pass_defines="forward"	defines=",USE_SOFT_SHADOWS">1</state>
		<state name="omni_shadow_jitter"	transparent="2"	title="Omni Shadow Jitter"		pass_defines="forward"	defines=",USE_OMNI_SHADOW_JITTER">1</state>
		<state name="lerp_cascades"			transparent="2"	title="Lerp Cascades"			pass_defines="forward"	defines=",USE_LERP_CASCADES">0</state>
		<state name="soft_shadows_quality"	transparent="2"	title="Soft Shadows Quality"	pass_defines="forward"	defines="SOFT_SHADOWS_LOW,SOFT_SHADOWS_MEDIUM,SOFT_SHADOWS_HIGH" items="low,medium,high">0</state>
	</group>
	
	<!--Parameters-->
	
	<group name="Default">
		<parameter name="material_mask"		deferred="1"	shared="0"	type="expression"	widget="mask24"		tooltip="&lt;p&gt;Material bitmask.&lt;/p&gt;&lt;p&gt;The decal will be projected onto a surface having this material assigned, if the material mask of the decal material matches this mask (one bit at least).&lt;/p&gt;">0xffffffff</parameter>
	</group>
	
	<group name="Base">
		<parameter name="albedo_color"		workflow="0"	shared="0"	type="color"	title="Albedo"			tooltip="Albedo color multiplier">1.0 1.0 1.0 1.0</parameter>
		<parameter name="metalness"			workflow="0"	shared="0"	type="slider"							tooltip="Metalness multiplier">0.0</parameter>
		<parameter name="roughness"			workflow="0"	shared="0"	type="slider"							tooltip="Roughness multiplier">1.0</parameter>
		<parameter name="specular"			workflow="0"	shared="0"	type="slider"	title="Specular"		tooltip="Specular multiplier">0.5</parameter>
		
		<parameter name="diffuse_color"		workflow="1"	shared="0"	type="color"	title="Diffuse"			tooltip="Diffuse color multiplier">1.0 1.0 1.0 1.0</parameter>
		<parameter name="specular_color"	workflow="1"	shared="0"	type="color"	title="Specular"		tooltip="Specular color multiplier">0.0 0.0 0.0 1.0</parameter>
		<parameter name="gloss"				workflow="1"	shared="0"	type="slider"	title="Gloss"			tooltip="Gloss multiplier">0.0</parameter>
		
		<parameter name="microfiber"								type="slider" 								tooltip="Microfiber multiplier">0.0</parameter>
		
		<parameter name="color" type="combiner"
			albedo_color="XYZW"
			diffuse_color="XYZW"
		/>
		
		<parameter name="shading" type="combiner"
			metalness="X" roughness="Y" specular="Z" microfiber="W"
			specular_color="XYZ" gloss="W"
		/>
		
		<parameter name="normal_scale"	type="slider" max="2.0" flags="max_expand" title="Normal Intensity" tooltip="Intensity of the normal texture.">1.0</parameter>
		<parameter name="translucent"	type="slider" title="Translucent" tooltip="Translucency multiplier.">0.0</parameter>
		
		<parameter name="transparent"		transparent="1,2" type="slider" max="10.0" flags="max_expand" title="Transparent Multiplier"	tooltip="Transparency multiplier.">1.0</parameter>
		<parameter name="transparent_pow"	transparent="1,2" type="slider" max="2.0" flags="max_expand" title="Transparent Pow"	tooltip="Transparency power.">1.0</parameter>
		
		<parameter name="opacity_fresnel"		transparent="1,2" type="slider" max="1.0"						title="Opacity Fresnel"		tooltip="">0.0</parameter>
		<parameter name="opacity_fresnel_pow"	transparent="1,2" type="slider" max="5.0" flags="max_expand"	title="Opacity Fresnel Pow"	tooltip="">2.0</parameter>
		
		<parameter name="uv_transform"										type="expression" title="UV Transform"		tooltip="UV transform." widget="uv">vec4(1.0f,1.0f,0.0f,0.0f)</parameter>
		<parameter name="ao_uv_transform"	ao_map="1"	ao_mapping="1,2"	type="expression" title="UV Transform AO"	tooltip="UV transform for Ambient Occlusion." widget="uv">vec4(1.0f,1.0f,0.0f,0.0f)</parameter>
		
		<parameter name="triplanar_blend"	base_mapping="2"	type="slider" title="Triplanar Blend" tooltip="Triplanar blending factor.">0.5</parameter>
		<parameter name="mip_bias"			mip_bias="1"		type="slider" min="-8.0" max="8.0" title="Mip Bias" tooltip="">0.0</parameter>
	</group>
	
	<group name="Detail">
		<parameter name="detail_albedo_color"	workflow="0"	detail="1"	shared="0"	type="color"	title="Albedo"		tooltip="Detail albedo color multiplier.">1.0 1.0 1.0 1.0</parameter>
		<parameter name="detail_metalness"		workflow="0"	detail="1"	shared="0"	type="slider"	title="Metalness"	tooltip="Detail metalness multiplier.">0.0</parameter>
		<parameter name="detail_roughness"		workflow="0"	detail="1"	shared="0"	type="slider"	title="Roughness"	tooltip="Detail roughness multiplier.">1.0</parameter>
		
		<parameter name="detail_specular"		workflow="0"	detail="1"	shared="0"	specular_map="1"	type="slider"	title="Specular"	tooltip="Detail specular multiplier.">0.5</parameter>
		<parameter name="detail_microfiber"		workflow="0"	detail="1"	shared="0"	microfiber_map="1"	type="slider"	title="Microfiber"	tooltip="Detail microfiber multiplier.">0.0</parameter>
		
		<parameter name="detail_diffuse_color"	workflow="1"	detail="1"	shared="0"	type="color"	title="Diffuse"		tooltip="Detail diffuse color multiplier.">1.0 1.0 1.0 1.0</parameter>
		<parameter name="detail_specular_color"	workflow="1"	detail="1"	shared="0"	type="color"	title="Specular"	tooltip="Detail specular color multiplier.">0.0 0.0 0.0 1.0</parameter>
		<parameter name="detail_gloss"			workflow="1"	detail="1"	shared="0"	type="slider"	title="Gloss"		tooltip="Detail gloss multiplier.">0.0</parameter>
		
		<parameter name="detail_color" type="combiner"
			detail_albedo_color="XYZW"
			detail_diffuse_color="XYZW"
		/>
		
		<parameter name="detail_shading" type="combiner"
			detail_metalness="X" detail_roughness="Y" detail_specular="Z" detail_microfiber="W"
			detail_specular_color="XYZ" detail_gloss="W"
		/>
		
		<parameter name="detail_uv_transform"			detail_mapping="1,2,3,4,5"	detail="1"	type="expression" widget="uv"	title="UV Transform"	tooltip="&lt;p&gt;UV transform for detail textures.&lt;/p&gt;">vec4(1.0f,1.0f,0.0f,0.0f)</parameter>
		<parameter name="detail_triplanar_blend"		detail_mapping="3"			detail="1"	type="slider"	tooltip="&lt;p&gt;Triplanar blending factor for detail textures.&lt;/p&gt;">0.5</parameter>
		
